{
  "name": "@hebo/gateway",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "engines": {
    "bun": ">=1.2.22"
  },
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --compile --minify-whitespace --minify-syntax --target=bun --outfile dist/server",
    "clean": "git clean -fdx -e '.env*' .",
    "dev": "bun run --watch src/index.ts | pino-pretty",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prestart": "bun run build",
    "start": "./dist/server"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.11",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@hebo/database": "workspace:*",
    "@hebo/shared-api": "workspace:*",
    "@hebo/shared-data": "workspace:*",
    "ai": "^5.0.19",
    "elysia": "^1.4.9",
    "sst": "^3.17.13",
    "voyage-ai-provider": "^2.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "bun-types": "^1.2.22",
    "pino-pretty": "^13.1.1"
  }
}
